import java.io.*;

public class HW_1 {
	
	static void prt(String s) {
		System.out.println(s);
	}
	
	public static void main(String[] args) {
		int month = Integer.parseInt(args[0]); 
		switch(month){
			case 1:
			case 3:
			case 5:
			case 7:
			case 8:
			case 10:
			case 12:{
				prt("31");
				break;
			}	
			case 4:
			case 6:
			case 9:
			case 11:{
				prt("30");
				break;
			}
			default: prt("28 or 29");
		}
	}
}
